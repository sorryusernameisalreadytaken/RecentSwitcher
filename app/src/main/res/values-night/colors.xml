<?xml version="1.0" encoding="utf-8"?>
<!--
    Overrides for colours when the device is using a dark (night) theme. In
    night mode the primary text colour should be light for good contrast.
-->
<resources>
    <!-- Use a light text colour on dark backgrounds. Provide explicit white for maximum contrast. -->
    <color name="recent_app_text_color">#FFFFFF</color>
    <color name="recent_app_text_color_excluded">#FF4444</color>
</resources>