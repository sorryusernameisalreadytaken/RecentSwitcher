<?xml version="1.0" encoding="utf-8"?>
<!--
    German translations for the RecentAppSwitcher project. When the device
    language is set to German (de), these strings will override the
    defaults defined in values/strings.xml.
-->
<resources>
    <!-- Application name -->
    <string name="app_name">RecentAppSwitcher</string>

    <!-- Button labels on the main screen -->
    <string name="show_recent_apps_button">Alle geöffneten Apps</string>
    <string name="open_last_app_button">Letzte App öffnen</string>
    <!-- German translation of the button that opens the system recents menu -->
    <string name="list_actions_button">System-Recents-Menü öffnen</string>
    <string name="manage_excluded_button">Ausgeschlossene Apps verwalten</string>
    <string name="collect_debug_button">Debug‑Bericht erstellen</string>
    <string name="show_live_events_button">Live‑Events</string>
    <string name="enable_service_button">Dienst aktivieren</string>

    <!-- Buttons for launching alternate recent apps list variants (German translations) -->
    <string name="show_recent_apps_variant1_button">Alle geöffneten Apps (V1)</string>
    <string name="show_recent_apps_variant2_button">Alle geöffneten Apps (V2)</string>
    <string name="show_recent_apps_variant3_button">Alle geöffneten Apps (V3)</string>
    <string name="show_recent_apps_variant4_button">Alle geöffneten Apps (V4)</string>
    <!-- Zusätzliche Variationen (V5–V8) für die Liste der letzten Apps -->
    <string name="show_recent_apps_variant5_button">Alle geöffneten Apps (V5)</string>
    <string name="show_recent_apps_variant6_button">Alle geöffneten Apps (V6)</string>
    <string name="show_recent_apps_variant7_button">Alle geöffneten Apps (V7)</string>
    <string name="show_recent_apps_variant8_button">Alle geöffneten Apps (V8)</string>

    <!-- Zusätzliche Varianten (V9–V12) für die Liste der zuletzt verwendeten Apps.
         Diese Versionen erweitern Varianten 2 und 3 um unterschiedliche
         Fokusreihenfolgen und Fokusierbarkeit der Zeile. Variante 9 nutzt
         den Fokusreihenfolgenmodus BEFORE_DESCENDANTS mit fokussierbarer
         Zeile und Zahnrad. Variante 10 nutzt AFTER_DESCENDANTS mit
         fokussierbarer Zeile und Zahnrad. Variante 11 macht die Zeile
         nicht fokussierbar, lässt aber das Zahnrad fokussierbar und
         verwendet BEFORE_DESCENDANTS. Variante 12 behält die Zeile als
         nicht fokussierbar bei, lässt das Zahnrad fokussierbar und nutzt
         AFTER_DESCENDANTS. Alle vier erlauben es, mit DPAD‑RIGHT zum
         Zahnrad zu gelangen und mit DPAD‑DOWN von dort zur nächsten
         App‑Zeile zu wechseln. -->
    <string name="show_recent_apps_variant9_button">Alle geöffneten Apps (V9)</string>
    <string name="show_recent_apps_variant10_button">Alle geöffneten Apps (V10)</string>
    <string name="show_recent_apps_variant11_button">Alle geöffneten Apps (V11)</string>
    <string name="show_recent_apps_variant12_button">Alle geöffneten Apps (V12)</string>

    <!-- Tabellenartige Varianten (V13–V14). Diese Varianten trennen die
         App‑Informationen und die Aktion in zwei Spalten. Variante 13
         verwendet BEFORE_DESCENDANTS als Fokusreihenfolge; Variante 14
         verwendet AFTER_DESCENDANTS. -->
    <string name="show_recent_apps_variant13_button">Alle geöffneten Apps (V13)</string>
    <string name="show_recent_apps_variant14_button">Alle geöffneten Apps (V14)</string>

    <!-- Experimentelle Varianten (V15–V22), die verschiedene Kombinationen aus
         Fokusierbarkeit der Zeile und des Zahnrads sowie der Fokus-Reihenfolge
         testen. Die Varianten sollen ein tabellarisches Layout mit klarer
         DPAD-Navigation ermöglichen. -->
    <string name="show_recent_apps_variant15_button">Alle geöffneten Apps (V15)</string>
    <string name="show_recent_apps_variant16_button">Alle geöffneten Apps (V16)</string>
    <string name="show_recent_apps_variant17_button">Alle geöffneten Apps (V17)</string>
    <string name="show_recent_apps_variant18_button">Alle geöffneten Apps (V18)</string>
    <string name="show_recent_apps_variant19_button">Alle geöffneten Apps (V19)</string>
    <string name="show_recent_apps_variant20_button">Alle geöffneten Apps (V20)</string>
    <string name="show_recent_apps_variant21_button">Alle geöffneten Apps (V21)</string>
    <string name="show_recent_apps_variant22_button">Alle geöffneten Apps (V22)</string>

